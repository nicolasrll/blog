{% extends "back/template.html.twig" %}
{% block title%}Liste des utilisateurs - Site personnel de Nicolas Rellier{% endblock %}
{% block content %}
    <div class="row justify-content-center">
        <h1 class="section-heading">Liste des utilisateurs</h1>
    </div>
    <div class="container">
    {% for user in users %}
        <p class="justify-content-center text-center">
            Id: {{ user.id }}<br>
            Login: {{ user.login }}<br>
            Compte: {{ user.role }}<br>
            Status: {{ user.status }}<br>
        </p>
    {% endfor %}
    </div>
{% endblock content %}
